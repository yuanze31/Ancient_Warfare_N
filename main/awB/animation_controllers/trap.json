      }
    },
    "controller.animation.trap_shackles": {
      "initial_state": "default",
      "states": {
        "default": {
          "on_entry": ["/effect @e[r=3] slowness 3 7 true"]
        }
      }
    },
    "controller.animation.trap_sleep": {
      "initial_state": "default",
      "states": {
        "default": {
          "on_entry": [
            "/effect @e[r=3] slowness 5 7 true",
            "/effect @e[r=3] weakness 5 255 true",
            "/effect @e[r=3] resistance 5 255 true"
          ]
        }
      }
    }
